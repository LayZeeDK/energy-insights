{
  "name": "energy-insights",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "engines": {
    "npm": "Use Yarn instead of NPM",
    "node": "16.17.0",
    "yarn": "1.22.19"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19"
  },
  "scripts": {
    "postinstall": "npm run decorate-angular-cli && npm run ngcc",
    "affected:build": "nx affected:build --configuration=production",
    "affected:ci": "yarn affected:build && yarn affected:lint && yarn affected:test && yarn affected:e2e --configuration=ci-production",
    "affected:e2e": "nx affected:e2e",
    "affected:lint": "nx affected:lint --max-warnings=0",
    "affected:test": "nx affected:test",
    "build": "nx build --configuration=production",
    "ci": "yarn build && yarn lint && yarn test && yarn e2e --configuration=ci-production",
    "decorate-angular-cli": "node ./decorate-angular-cli.js",
    "e2e": "nx e2e energy-insights-app-e2e",
    "lint": "nx run-many --target=lint --all --max-warnings=0",
    "lint-fix": "nx run-many --target=lint --all --fix",
    "ng": "nx",
    "ngcc": "ngcc --properties es2015 browser module main",
    "ngcc:app": "npm run ngcc -- --tsconfig apps/energy-insights-app/tsconfig.app.json --use-program-dependencies",
    "nx": "nx",
    "start": "nx serve",
    "test": "nx run-many --target=test --all"
  },
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/platform-browser": "13.3.11",
    "@angular/router": "13.3.11",
    "@ngrx/component-store": "~13.0.0",
    "@nrwl/angular": "13.10.6",
    "@rx-angular/cdk": "1.0.0-rc.1",
    "@rx-angular/template": "1.0.0-rc.1",
    "luxon": "~2.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.9",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "13.3.9",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@ngworker/spectacular": "~0.5.0",
    "@nrwl/cli": "13.9.2",
    "@nrwl/cypress": "13.10.6",
    "@nrwl/eslint-plugin-nx": "13.10.6",
    "@nrwl/jest": "13.10.6",
    "@nrwl/linter": "13.10.6",
    "@nrwl/nx-cloud": "14.6.0",
    "@nrwl/workspace": "13.10.6",
    "@schematics/angular": "~13.2.0",
    "@testing-library/angular": "~11.0.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "~5.16.2",
    "@types/jest": "27.0.2",
    "@types/luxon": "~2.3.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "cypress": "^9.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-ngrx": "~2.0.4",
    "eslint-plugin-sonarjs": "~0.12.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "nx": "13.10.6",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "4.6.4"
  }
}
