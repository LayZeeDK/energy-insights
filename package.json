{
  "name": "energy-insights",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "engines": {
    "npm": "Use Yarn instead of NPM",
    "node": "14.19.0",
    "yarn": "1.22.17"
  },
  "volta": {
    "node": "14.19.0",
    "yarn": "1.22.17"
  },
  "scripts": {
    "postinstall": "node ./decorate-angular-cli.js && ngcc --first-only --properties es2015 module fesm2015 esm2015 browser main --create-ivy-entry-points --tsconfig apps/energy-insights-app/tsconfig.app.json --use-program-dependencies",
    "build": "nx build",
    "ci": "yarn build && yarn lint && yarn test && yarn e2e",
    "e2e": "nx e2e energy-insights-app-e2e",
    "lint": "nx run-many --target=lint --all",
    "lint-fix": "nx run-many --target=lint --all --fix",
    "ng": "nx",
    "nx": "nx",
    "start": "nx serve",
    "test": "nx run-many --target=test --all"
  },
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@ngrx/component-store": "~13.0.0",
    "@nrwl/angular": "13.9.2",
    "luxon": "~2.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@nrwl/cli": "13.9.2",
    "@nrwl/cypress": "13.9.2",
    "@nrwl/eslint-plugin-nx": "13.9.2",
    "@nrwl/jest": "13.9.2",
    "@nrwl/linter": "13.9.2",
    "@nrwl/nx-cloud": "13.1.6",
    "@nrwl/workspace": "13.9.2",
    "@schematics/angular": "~13.2.0",
    "@types/jest": "27.0.2",
    "@types/luxon": "~2.3.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "cypress": "^9.1.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-ngrx": "~2.0.4",
    "eslint-plugin-sonarjs": "~0.12.0",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "nx": "13.9.2",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  }
