{
  "name": "energy-insights",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "engines": {
    "npm": "Use Yarn instead of NPM",
    "node": "16.17.0",
    "yarn": "1.22.19"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19"
  },
  "scripts": {
    "postinstall": "npm run decorate-angular-cli && npm run ngcc",
    "affected:build": "nx affected:build --configuration=production",
    "affected:ci": "yarn affected:build && yarn affected:lint && yarn affected:test && yarn affected:e2e --configuration=ci-production",
    "affected:e2e": "nx affected:e2e",
    "affected:lint": "nx affected:lint --max-warnings=0",
    "affected:test": "nx affected:test",
    "build": "nx build --configuration=production",
    "ci": "yarn build && yarn lint && yarn test && yarn e2e --configuration=ci-production",
    "decorate-angular-cli": "node ./decorate-angular-cli.js",
    "e2e": "nx e2e energy-insights-app-e2e",
    "lint": "nx run-many --target=lint --all --max-warnings=0",
    "lint-fix": "nx run-many --target=lint --all --fix",
    "ng": "nx",
    "ngcc": "ngcc --properties es2015 browser module main",
    "ngcc:app": "npm run ngcc -- --tsconfig apps/energy-insights-app/tsconfig.app.json --use-program-dependencies",
    "nx": "nx",
    "start": "nx serve",
    "test": "nx run-many --target=test --all"
  },
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/router": "14.2.0",
    "@ngrx/component-store": "14.0.2",
    "@nrwl/angular": "14.6.1",
    "@rx-angular/cdk": "1.0.0-rc.1",
    "@rx-angular/template": "1.0.0-rc.1",
    "luxon": "~2.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.1",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "14.2.0",
    "@angular/language-service": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@ngworker/spectacular": "~0.5.0",
    "@nrwl/cli": "14.6.1",
    "@nrwl/cypress": "14.6.1",
    "@nrwl/eslint-plugin-nx": "14.6.1",
    "@nrwl/jest": "14.6.1",
    "@nrwl/linter": "14.6.1",
    "@nrwl/nx-cloud": "14.6.0",
    "@nrwl/workspace": "14.6.1",
    "@schematics/angular": "14.2.1",
    "@testing-library/angular": "~11.0.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "~5.16.2",
    "@types/jest": "28.1.8",
    "@types/luxon": "~2.3.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "cypress": "^9.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-ngrx": "~2.0.4",
    "eslint-plugin-sonarjs": "~0.12.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.2",
    "nx": "14.6.1",
    "prettier": "2.6.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
