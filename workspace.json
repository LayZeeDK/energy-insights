{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "inlineStyle": true,
      "inlineTemplate": true,
      "linter": "eslint",
      "prefix": "nrg",
      "skipTests": true,
      "strict": true,
      "style": "scss",
      "tags": "type:app",
      "unitTestRunner": "jest",
      "viewEncapsulation": "None"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "displayBlock": true
    }
  },
  "projects": {}
}
